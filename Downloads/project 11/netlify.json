{
  "build": {
    "command": "./build.sh",
    "publish": "dist",
    "environment": {
      "NODE_VERSION": "18"
    }
  },
  "redirects": [
    {
      "from": "/",
      "to": "/index.html",
      "status": 200
    },
    {
      "from": "/*",
      "to": "/index.html",
      "status": 200
    }
  ]
}
